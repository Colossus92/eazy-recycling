{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "typescript-axios": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/api/client",
        "inputSpec": "#{cwd}/../../docs/openapi/spec.yaml",
        "templateDir": "#{cwd}/templates/typescript-axios",
        "additionalProperties": {
          "apiPackage": "apis",
          "modelPackage": "models",
          "withSeparateModelsAndApi": "true"
        },
        "schemaMappings": {
          "HttpStatus": "HttpStatusEnum"
        }
      }
    }
  }
}
